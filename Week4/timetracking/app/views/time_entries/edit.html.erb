<h3>Edit <%= @project.name %>'s Time Entries</h3>

<%= form_for [@project, @entry] do |f| %>
  <%= f.label :hours %>
  <%= f.text_field :hours %>
  <br>
  <%= f.label :minutes %>
  <%= f.text_field :minutes %>
  <br>
  <%= f.label :date %>
  <%= f.date_field :date%>

  <%= f.submit 'Update' %>
<% end %>
