<%= form_for @concert do |f| %>
  <%= f.label :artist %>
  <%= f.text_field :artist %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :price %>
  <%= f.number_field :price %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :venue %>
  <%= f.text_field :venue %>

  <%= f.label :date %>
  <%= f.date_field :date %>

  <%= f.button %>
<% end %>